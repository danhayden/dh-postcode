{
  "name": "dh-postcode",
  "version": "1.0.2",
  "description": "simple uk postcode validation and formatting",
  "main": "dist/postcode.js",
  "module": "dist/postcode.es.js",
  "umd:main": "dist/postcode.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "pretest": "standard",
    "test": "jest",
    "lint": "standard --format",
